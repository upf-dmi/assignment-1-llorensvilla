# 游늷 Funci칩n para eliminar valores at칤picos (outliers) utilizando el rango intercuartil (IQR)
remove_outliers_column <- function(x) {
# Calcular el rango intercuartil (IQR) de la variable
iqr_val <- IQR(x, na.rm = TRUE)
# Definir los l칤mites inferior y superior para considerar un valor como at칤pico
lower_bound <- quantile(x, 0.25, na.rm = TRUE) - 1.5 * iqr_val
upper_bound <- quantile(x, 0.75, na.rm = TRUE) + 1.5 * iqr_val
# Reemplazar los valores que est치n fuera de los l칤mites por los valores l칤mite m치s cercanos
x[x < lower_bound] <- lower_bound
x[x > upper_bound] <- upper_bound
# Retornar la variable sin outliers
return(x)
}
